{
  "name": "storj",
  "version": "0.0.0",
  "description": "implementation of the storj protocol for node.js and the browser",
  "main": "index.js",
  "directories": {
    "test": "test",
    "lib": "lib",
    "doc": "doc",
    "example": "example"
  },
  "scripts": {
    "test": "npm run testsuite && npm run linter",
    "testsuite": "./node_modules/.bin/mocha test/** --recursive",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive",
    "linter": "./node_modules/.bin/jshint --config .jshintrc ./index.js ./lib ./test",
    "build": "./node_modules/.bin/browserify index.js -s storj -o dist/storj.browser.js",
    "makedoc": "./node_modules/.bin/jsdoc index.js lib -r -p -R README.md -c .jsdoc.json --verbose -d ./doc"
  },
  "keywords": [
    "storj",
    "p2p"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/storj/storjnode-js.git"
  },
  "author": "Gordon Hall <gordon@gordonwritescode.com>",
  "contributors": [],
  "license": "LGPL-3.0",
  "engines": {
    "node": "4.x.x"
  },
  "dependencies": {
    "jsen": "^0.6.0",
    "kad": "^1.3.6",
    "merge": "^1.2.0",
    "ms": "^0.7.1"
  },
  "devDependencies": {
    "browserify": "^11.1.0",
    "chai": "^2.2.0",
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "jsdoc": "^3.4.0",
    "jshint": "2.8.0",
    "minami": "^1.1.1",
    "mocha": "^2.2.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.14.1"
  }
}
