    "gh-pages": "^0.9.0",
    "ink-docstrap": "github:bookchin/docstrap#f175ba82cd7175e851304c6df3db1d37424b7545",
    "istanbul": "^0.4.5",
    "jsdoc": "^3.6.3",
    "mocha": "^6.1.4",
    "node-mocks-http": "^1.7.6",
    "noisegen": "^1.0.0",
    "proxyquire": "^1.7.3",
    "sinon": "^7.3.2"
  }
}
